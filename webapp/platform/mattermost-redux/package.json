{
  "name": "mattermost-redux",
  "version": "9.3.0",
  "description": "Redux utilities for working with Mattermost",
  "keywords": [
    "mattermost"
  ],
  "homepage": "https://github.com/mattermost/mattermost/tree/master/webapp/platform/mattermost-redux#readme",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "exports": {
    "./*": "./lib/*"
  },
  "typesVersions": {
    ">=3.1": {
      "*": [
        "./lib/*",
        "./lib/index.*"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "github:mattermost/mattermost",
    "directory": "webapp/platform/mattermost-redux"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.38",
    "redux": "^4.2.0",
    "redux-batched-actions": "^0.5.0",
    "redux-thunk": "^2.4.2",
    "rudder-sdk-js": "^1.0.16",
    "shallow-equals": "^1.0.0",
    "timezones.json": "^1.6.1"
  },
  "devDependencies": {
    "@types/jest": "28.1.8",
    "identity-obj-proxy": "3.0.0",
    "isomorphic-fetch": "3.0.0",
    "jest": "27.1.0",
    "jest-environment-jsdom": "29.7.0",
    "nock": "13.2.8",
    "@redux-devtools/extension": "3.2.3",
    "redux-mock-store": "1.5.4",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@mattermost/types": "9.3.0",
    "@mattermost/client": "9.3.0",
    "typescript": "^4.3.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "scripts": {
    "build": "tsc --build tsconfig.build.json --verbose",
    "check": "eslint --ext .js,.jsx,.tsx,.ts ./src --quiet",
    "run": "tsc --watch --preserveWatchOutput",
    "test": "cross-env TZ=Etc/UTC jest",
    "test-ci": "cross-env TZ=Etc/UTC jest --ci --forceExit --detectOpenHandles --maxWorkers=100%",
    "clean": "rm -rf lib node_modules *.tsbuildinfo"
  }
}
